            <Button fx:id="yesButton" mnemonicParsing="false" opacity="0.8" prefWidth="75.0" text="Sim">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="noButton" mnemonicParsing="false" opacity="0.8" prefWidth="75.0" text="Não">
               <font>
                  <Font size="14.0" />
