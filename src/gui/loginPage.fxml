            <HBox prefHeight="6.0" prefWidth="279.0">
               <children>
                  <Button fx:id="button1" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="26.0" prefWidth="99.0" text="OK" />
                  <Label alignment="BASELINE_RIGHT" prefHeight="17.0" prefWidth="140.0" text="Adm">
                     <padding>
