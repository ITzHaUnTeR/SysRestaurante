<VBox alignment="CENTER" prefHeight="466.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
   <children>
      <VBox alignment="CENTER" prefHeight="469.0" prefWidth="379.0" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField promptText="Login" />
